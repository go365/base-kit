{
  "name": "go365/base-kit",
  "version": "1.0.0",
  "description": "獨立工具包",
  "type": "library",
  "authors": [
    {
      "name": "go365",
      "email": "go365.cc@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Base\\Kit\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Base\\Kit\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^7"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
